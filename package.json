{
  "name": "automationplaywrighttests",
  "version": "1.0.0",
  "description": "Autotests using the Playwright framework",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "test:qa": "npx playwright test --project=QA",
    "test:dev": "npx playwright test --project=DEV",
    "test:stage": "npx playwright test --project=STAGE",
    "test:headed": "npx playwright test --headed",
    "report": "npx playwright show-report",
    "lint": "eslint . --ext .js,.ts"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Interfirst/AutomationPlaywrightTests.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Interfirst/AutomationPlaywrightTests/issues"
  },
  "homepage": "https://github.com/Interfirst/AutomationPlaywrightTests#readme",
  "devDependencies": {
    "@playwright/test": "^1.24.2",
    "eslint": "8.17.0",
    "husky": "8.0.0"
  },
  "dependencies": {
    "dotenv": "16.0.1"
  }
}
